        <dc:title>Irish Fairy and Folk Tales</dc:title>
        <dc:creator opf:file-as="Yeats, William Butler &amp; Muldoon, Paul" opf:role="aut">William Butler Yeats</dc:creator>
        <dc:creator opf:file-as="Yeats, William Butler &amp; Muldoon, Paul" opf:role="aut">Paul Muldoon</dc:creator>
